{
	"name": "@cloudflare/prerelease-registry",
	"version": "0.0.2",
	"private": true,
	"main": "dist/index.js",
	"scripts": {
		"build": "npx wrangler pages functions build --fallback-service='' ./functions/routes --outdir=dist",
		"check:type": "tsc",
		"lint": "eslint",
		"prepublish": "npm run build",
		"publish": "npx wrangler deploy dist/index.js",
		"prestart": "npm run build",
		"start": "npx wrangler dev dist/index.js"
	},
	"dependencies": {
		"eslint-worker-config": "*",
		"jszip": "^3.7.1",
		"tsconfig": "*",
		"wrangler": "*"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20221111.1",
		"typescript": "^4.5.5"
	}
}
